#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize

  if [[ "$#" == 0 ]]; then
    set -- "$SHELL"
  fi

  eval $(fogg account tf env fogg_ | perl -pe 's{^}{export };s{=}{="};s{$}{"}' | sort)
  export AWS_REGION="${fogg__region}"
  export AWS_DEFAULT_REGION="${fogg__region}"
  exec fogg account "$@"
}

source sub "$BASH_SOURCE" "$@"
